<?xml version="1.0" encoding="utf-8"?>

<!-- ZigZag Command -->

<TurtleInstructionSet xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns="commands.*">

	<SetRotation value="45" />
	
	<Repeat times="3">
		<Forward distance="50" />
		<Turn angle="-90" />
		<Forward distance="50" />
		<Turn angle="90" />	
	</Repeat>
	
	<Turn angle="-45" />
	
</TurtleInstructionSet>